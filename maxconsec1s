class Solution:
    def findMaxConsecutiveOnes(self, nums: List[int]) -> int:
        maxi=-997884
        count=0
        for i in range(len(nums)):
            if(nums[i]==1):
                count+=1
            else:
                count=0
            if maxi<count:
                maxi=count
           
        return (maxi)
           
            
            
        
